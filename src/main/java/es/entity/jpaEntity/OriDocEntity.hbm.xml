<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="es.entity.jpaEntity.OriDocEntity" table="ori_doc" schema="esWeb">
        <id name="id" column="id"/>
        <property name="location" column="location"/>
        <property name="upTime" column="up_time"/>
        <property name="saveTime" column="save_time"/>
        <property name="isSave" column="is_save"/>
        <property name="isDel" column="is_del"/>
        <property name="uploader" column="uploader"/>
    </class>
</hibernate-mapping>