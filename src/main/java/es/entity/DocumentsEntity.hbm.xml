<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="es.entity.DocumentsEntity" table="documents" schema="es">
        <id name="title">
            <column name="title" sql-type="varchar(45)" length="45"/>
        </id>
        <property name="uploaded">
            <column name="uploaded" sql-type="tinyint(3) unsigned"/>
        </property>
        <property name="deleted">
            <column name="deleted" sql-type="tinyint(3) unsigned"/>
        </property>
    </class>
</hibernate-mapping>